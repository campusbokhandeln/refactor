{
    "name": "campusbokhandeln/refactor",
    "description": "Interview package",
    "keywords": [
        "campusbokhandeln",
        "refactor"
    ],
    "homepage": "https://github.com/campusbokhandeln/refactor",
    "license": "MIT",
    "authors": [
        {
            "name": "Isak Berglind",
            "email": "info@isakberglind.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Campusbokhandeln\\Refactor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Campusbokhandeln\\Refactor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
